{
  "redirects": [
    {
      "source": "/projects/en/:rest*",
      "destination": "/projects/:rest*",
      "permanent": true
    },
    {
      "source": "/es/projects/es/:rest*",
      "destination": "/es/projects/:rest*",
      "permanent": true
    },
    {
      "source": "/projects/xr-training-apk-unity-vuforia/:rest*",
      "destination": "/projects/xr-training-apk-unity/:rest*",
      "permanent": true
    },
    {
      "source": "/projects/en/:rest*",
      "destination": "/projects/:rest*",
      "permanent": true
    },
    {
      "source": "/es/projects/es/:rest*",
      "destination": "/es/projects/:rest*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
